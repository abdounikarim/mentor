{% xlsdocument %}
    {% xlssheet 'Sessions' %}
        {% xlsrow %}
            {% xlscell { style: { font: { size: '18' } } } %}Date{% endxlscell %}
            {% xlscell { style: { font: { size: '18' } } } %}Mentor√©{% endxlscell %}
            {% xlscell { style: { font: { size: '18' } } } %}Parcours{% endxlscell %}
            {% xlscell { style: { font: { size: '18' } } } %}Projet{% endxlscell %}
            {% xlscell { style: { font: { size: '18' } } } %}Type{% endxlscell %}
            {% xlscell { style: { font: { size: '18' } } } %}No-show{% endxlscell %}
            {% xlscell { style: { font: { size: '18' } } } %}Tarif{% endxlscell %}
        {% endxlsrow %}
        {% for sessions in sessionsData %}
            {% xlsrow %}
            {% xlscell %}{{ session.date }}{% endxlscell %}
            {% xlscell %}{{ session.student.firstname }} {{ session.student.lastname }}{% endxlscell %}
            {% xlscell %}{{ session.student.path.name }}{% endxlscell %}
            {% xlscell %}{{ session.project.name }}{% endxlscell %}
            {% xlscell %}{{ session.type }}{% endxlscell %}
            {% xlscell %}{% if session.noshow == true %}Oui{% else %}{{ session.noshow }}{% endif %}{% endxlscell %}
            {% xlscell %}{{ session.price }}{% endxlscell %}
            {% endxlsrow %}
        {% endfor %}
    {% endxlssheet %}
    {% xlssheet 'Facture' %}
    {% endxlssheet %}
{% endxlsdocument %}
